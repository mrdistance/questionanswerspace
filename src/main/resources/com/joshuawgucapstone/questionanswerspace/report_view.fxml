<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.joshuawgucapstone.questionanswerspace.ReportController">
   <children>
      <Label layoutX="87.0" layoutY="14.0" text="REPORTS">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="searchBox" layoutX="351.0" layoutY="19.0" promptText="Quiz name..." />
      <TableView fx:id="reportsTable" layoutX="22.0" layoutY="53.0" prefHeight="300.0" prefWidth="561.0">
        <columns>
          <TableColumn fx:id="timestamp" prefWidth="155.0" text="TimeStamp" />
          <TableColumn fx:id="quizName" prefWidth="219.0" text="Quiz Name" />
            <TableColumn fx:id="username" prefWidth="104.0" text="Username" />
            <TableColumn fx:id="score" prefWidth="82.0" text="Score" />
        </columns>
      </TableView>
      <Button layoutX="519.0" layoutY="19.0" mnemonicParsing="false" onAction="#search" text="SEARCH" />
      <Button layoutX="526.0" layoutY="361.0" mnemonicParsing="false" onAction="#back" text="BACK" />
   </children>
</AnchorPane>
